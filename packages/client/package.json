    "class-transformer": "0.5.1",
    "dom-to-image": "2.6.0",
    "immer": "10.1.1",
    "lodash.clonedeep": "4.5.0",
    "lodash.debounce": "4.0.8",
    "lodash.isequal": "4.5.0",
    "next": "15.4.6",
    "oxide.ts": "1.1.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "reflect-metadata": "0.2.2",
    "socket.io-client": "4.8.1",
